{
  "name": "fullstack_react_express_boiler_plate",
  "version": "1.0.0",
  "description": "My simple boiler plate for my project",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "npm run build && node src/server/server.js ",
    "client": "webpack-dev-server --open --hot --config webpack.dev.js",
    "server": "nodemon src/server/server.js",
    "dev": "concurrently \"npm run server \" \"npm run client \""
  },
  "author": "Liuk Jhivran Tulawie",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "nodemon": "^2.0.3",
    "style-loader": "^1.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
